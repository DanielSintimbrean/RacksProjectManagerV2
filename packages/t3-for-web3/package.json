{
  "name": "t3-for-web3",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "precommit": "lint-staged",
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate && typechain --target=ethers-v5 --out-dir=src/smart-contracts/types src/smart-contracts/abi/*.json",
    "next-lint": "next lint",
    "check-types": "tsc --noemit",
    "eslint": "eslint .",
    "lint": "npm run eslint && npm run check-types",
    "start": "next start",
    "prisma:reset": "prisma db push --force-reset",
    "prisma:seed": "prisma db push --force-reset && prisma db seed"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@headlessui/react": "^1.7.8",
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.8.1",
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@tanstack/query-core": "^4.20.4",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "@typechain/ethers-v5": "^10.2.0",
    "daisyui": "^2.49.0",
    "ethers": "^5.7.2",
    "iron-session": "^6.3.1",
    "my-hardhat": "*",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "siwe": "^2.1.3",
    "superjson": "1.12.2",
    "tozod": "^3.0.0",
    "typechain": "^8.1.1",
    "wagmi": "^0.10.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.1.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.8.1",
    "tailwindcss": "^3.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.6"
  }
}
